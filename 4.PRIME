import java.util.Scanner;
public class Prime{
    public static void main(String args[])
    {
        int n,i,c=0;
        Scanner scan=new Scanner(System.in);
        System.out.println("value of number");
        n=scan.nextInt();
        for(i=1;i<=n;i++)
        {
        if(n%i==0)
        {
          c++;
        }
        }
if(c==2)
{
  System.out.println("prime number");  
}
else
{
   System.out.println(" not prime number");   
}
    }
    
}
